<%= form_with model: @evaluated, local: true do |form| %>
  <div class="field">
    <%= form.label :nome %>
    <%= form.text_field :nome %>
  </div>

  <div class="field">
    <%= form.label :cpf %>
    <%= form.text_field :cpf %>
  </div>

  <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div class="field">
    <%= form.label :data_de_nascimento %>
    <%= form.date_field :data_de_nascimento %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
